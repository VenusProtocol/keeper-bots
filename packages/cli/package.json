{
  "name": "@venusprotocol/cli",
  "version": "1.6.1-dev.1",
  "license": "MIT",
  "bin": {
    "venus": "dist/venus.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "ava",
    "lint": "yarn lint:ts && yarn prettier:check",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:ts:fix": "eslint --fix --ext .js,.ts .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@graphprotocol/client-cli": "3.0.0",
    "@graphql-mesh/cache-localforage": "^0.98.4",
    "@graphql-mesh/cli": "^0.90.5",
    "@graphql-mesh/config": "^0.100.5",
    "@graphql-mesh/graphql": "^0.98.4",
    "@venusprotocol/keeper-bots": "1.1.0",
    "@venusprotocol/venus-protocol": "^9.1.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "ink": "^4.1.0",
    "pastel": "^2.0.0",
    "react": "^18.2.0",
    "viem": "^2.10.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/react": "^18.2.55",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^2.8.7",
    "semantic-release": "^23.1.1",
    "semantic-release-monorepo": "^8.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "xo": "^0.54.2"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "publishConfig": {
    "access": "public",
    "provenance": false
  }
}
